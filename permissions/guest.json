{
  "role_info": {
    "name": "guest",
    "display_name": "Guest",
    "description": "Guest Role when non-register user enters",
    "role_type": "default",
    "priority": 0,
    "is_system": true
  },
  "permissions": [
    {
      "route": "/auth/login",
      "methods": ["POST"],
      "description": "Login to system"
    },
    {
      "route": "/auth/register",
      "methods": ["POST"],
      "description": "Register the user"
    },
    {
      "route":"/request",
      "methods": ["POST", "GET"],
      "description": "Make request for a role assignment"
    },
    {
      "route":"/request/status",
      "methods":["GET"],
      "description":"Get the status of the request for the role assignment"
    }
  ]
}
